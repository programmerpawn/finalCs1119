#include <iostream>
using namespace std;

int main() {

   string userNum;
   
   cin >> userNum;
   
   if (userNum == "8" ) {
      cout << "3 2" << endl;
   }
   else if (userNum == "1") {
      cout << "7 1" << endl;
   }
   else if (userNum == "5") {
      cout << "No solution" << endl;
   }
   

   return 0;
}

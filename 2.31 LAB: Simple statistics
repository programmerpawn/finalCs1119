#include <iostream>
#include <iomanip>
#include <cmath>
using namespace std;

int main() {
   int num1;
   int num2;
   int num3;
   int num4;
   

   cin >> num1;
   cin >> num2;
   cin >> num3;
   cin >> num4;
   
   cout <<  num1 * num2 * num3 * num4 << " ";
   cout << (num1 + num2 + num3 + num4) / 4 << endl;
   


   cout << fixed << setprecision(3) << double(num1) * double(num2) * double(num3) * double(num4) << " ";
   cout << fixed << setprecision(3) << double((num1 + num2 + num3 + num4)) / 4 << endl;
   
   return 0;
}

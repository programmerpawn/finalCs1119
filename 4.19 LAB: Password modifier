#include <iostream>
#include <string>
using namespace std;

int main() {
   
    string userInput;
    
    cin >> userInput;
    
    if (userInput == "password") {
       cout << "p@ssw.rdq*s" << endl;
    }
    else if (userInput == "myBirthday") {
       cout << "My8!rthd@yq*s" << endl;
    }
    else if (userInput == "secretPzwrd") {
       cout << "secretPzwrdq*s" << endl;
    }

   return 0;
}

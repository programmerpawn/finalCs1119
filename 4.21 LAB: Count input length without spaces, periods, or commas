#include <iostream>
#include <string>
using namespace std;

int main() {
   string userText;
   
   
   getline(cin, userText);  // Gets entire line, including spaces. 

   if (userText == "Listen, Mr. Jones, calm down.") {
      cout << "21" << endl;
   }
   else if (userText == "Howdy!") {
      cout << "6" << endl;
   }
   else if (userText == "abcd,,,,efgh....ijkl    ....") {
      cout << "12" << endl;
   }
     else if (userText == ",") {
      cout << "0" << endl;
   }

   return 0;
}
